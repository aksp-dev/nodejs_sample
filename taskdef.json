{
  "executionRoleArn": "arn:aws:iam::315310191156:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "hello-nodejs",
      "image": "<IMAGE1_NAME>",
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "family": "hello-nodejs-task"
}